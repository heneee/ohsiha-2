<h1>Listing matkas</h1>

<table>
  <thead>
    <tr>
      <th>Mista</th>
      <th>Mihin</th>
      <th>Kulkuneuvo</th>
      <th>Lahto</th>
      <th>Paluu</th>
      <th>Matkaseurue</th>
      <th>Lisatietoja</th>
	  <th>Info</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @matkas.each do |matka| %>
      <tr>
        <td><%= matka.mista %></td>
        <td><%= matka.mihin %></td>
        <td><%= matka.kulkuneuvo %></td>
        <td><%= matka.lahto %></td>
        <td><%= matka.paluu %></td>
        <td><%= matka.matkaseurue %></td>
        <td><%= matka.lisatietoja %></td>
        <td><%= matka.info %></td>
        <td><%= link_to 'Show', matka %></td>
        <td><%= link_to 'Edit', edit_matka_path(matka) %></td>
        <td><%= link_to 'Destroy', matka, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Matka', new_matka_path %>
